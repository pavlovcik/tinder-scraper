{
  "private": true,
  "scripts": {
    "start": "node ./dist infinity",
    "start:gangnam": "node ./dist infinity --latitude 37.524415 --longitude 127.049327",
    "build:clean": "rm -rf ./dist",
    "build:compile": "npx tsc",
    "build:compile-watch": "npx tsc --watch",
    "build": "npx run-s build:clean build:compile-watch"
  },
  "main": "dist/index.js",
  "dependencies": {
    "minimist": "^1.2.5",
    "p-all": "^3.0.0",
    "p-any": "^3.0.0",
    "p-map": "^4.0.0",
    "promise-any-polyfill": "^0.1.2",
    "puppeteer": "^2.1.1",
    "puppeteer-extra": "^3.1.15",
    "puppeteer-extra-plugin-stealth": "^2.6.2",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9"
  },
  "devDependencies": {
    "@types/puppeteer": "^2.0.1",
    "@types/request-promise-native": "^1.0.17",
    "npm-run-all": "^4.1.5",
    "source-map-support": "^0.5.18",
    "ts-node": "^8.9.0",
    "typescript": "^4.0.3"
  },
  "nodemonConfig": {
    "events": {
      "start": "echo \"\\x1Bc\""
    },
    "restartable": "rs",
    "ignore": [
      ".git",
      "node_modules/**/node_modules"
    ],
    "verbose": true,
    "execMap": {
      "js": "node --harmony"
    },
    "watch": [
      "./dist/tsconfig.tsbuildinfo"
    ],
    "env": {
      "NODE_ENV": "development"
    },
    "ext": "js"
  }
}